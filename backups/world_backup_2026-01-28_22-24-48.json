{
  "dialogs": [
    {
      "id": "ab0658ed-5591-4822-9e07-c182b2008afa",
      "name": "Oldtown Guard 1",
      "nodeId": "d-oldtownguard-main",
      "text": "Hi stranger, how can i help you?",
      "alternateTexts": [
        "Good day to you, what do you want?"
      ],
      "options": [
        {
          "nodeId": "ask_inn",
          "text": "Where is the next Inn? I need a beer!",
          "answer": {
            "nodeId": "answer_griphon",
            "text": "You can reach the Old Feisty Griphon Inn directly from Oldtowns main street! You cant miss it ",
            "created": "0001-01-01T00:00:00Z"
          },
          "created": "0001-01-01T00:00:00Z"
        },
        {
          "nodeId": "merchant",
          "text": "I need restock, where can i find the local merchant?",
          "answer": {
            "nodeId": "a_market_merchant",
            "text": "You can find the next merchant right here on the market",
            "options": [
              {
                "nodeId": "d-oldtownguard-main",
                "text": "New option",
                "created": "0001-01-01T00:00:00Z"
              }
            ],
            "created": "0001-01-01T00:00:00Z"
          },
          "created": "0001-01-01T00:00:00Z"
        },
        {
          "nodeId": "mayor",
          "text": "What you can tell me about the Mayor?",
          "answer": {
            "nodeId": "answer_1768868361649",
            "text": "What do you want to know?",
            "options": [
              {
                "nodeId": "option_1768868373460",
                "text": "How long has he been mayor in this town?",
                "answer": {
                  "nodeId": "A_1738324",
                  "text": "Not for long to be honest he is rather new in town",
                  "options": [
                    {
                      "nodeId": "d-oldtownguard-main",
                      "text": "New option",
                      "created": "0001-01-01T00:00:00Z"
                    }
                  ],
                  "is_dialog_exit": false,
                  "created": "0001-01-01T00:00:00Z"
                },
                "created": "0001-01-01T00:00:00Z"
              }
            ],
            "created": "0001-01-01T00:00:00Z"
          },
          "created": "0001-01-01T00:00:00Z"
        },
        {
          "nodeId": "exit",
          "text": "Thanks that is all for now",
          "is_dialog_exit": true,
          "created": "0001-01-01T00:00:00Z"
        }
      ],
      "created": "2026-01-20T01:05:52.614294479+01:00"
    }
  ],
